    "diagramCtrl" : {
      "type" : "expression",
      "value" : [
        "return {",
        "    getDiagramTitle: function() {",
        "        if (!$data.currentProcessUrl) {",
        "            return \"\";",
        "        }",
        "        return uiTranslate(\"Process\") + \": \" + $data.currentProcessUrl.displayName + \" (\" + $data.currentProcessUrl.version + \")\";",
        "    }",
        "};"
      ],
      "exposed" : false
    },
    "processId" : {
      "type" : "expression",
      "value" : [
        "if (!$data.urlId || $data.urlId === \"\") {",
        "    return undefined;",
        "}",
        "",
        "return $data.urlId;"
      ],
      "exposed" : false
    },
    "invalidProcessResponse" : {
      "type" : "expression",
      "value" : [
        "return !$data.processId || $data.processResponseStatus !== 200;"
      ],
      "exposed" : false
    },
    "errorMessage" : {
      "type" : "expression",
      "value" : [
        "",
        "let header = '<div class=\"alert alert-info\" translate><i class=\"glyphicon glyphicon-info-sign\" aria-hidden=\"true\"></i> ';",
        "let cannotDisplay = uiTranslate(\"Cannot display BPMN diagram.\");",
        "",
        "if (!$data.processId) {",
        "    return  header + cannotDisplay + \"&nbsp;\" + uiTranslate(\"Process id is missing.\") + \"</div>\";",
        "}",
        "",
        "if ($data.invalidProcessResponse) {",
        "    return  header + cannotDisplay + \"&nbsp;\" + uiTranslate(\"No process found with id:\") + \"&nbsp;<b>\" + $data.processId + \"</b></div>\";",
        "}",
        "",
        "return \"\";"
      ],
      "exposed" : false
    },
    "headerMessage" : {
      "type" : "constant",
      "value" : [
        "This page displays the active flow nodes of all open cases of the process."
      ],
      "exposed" : false
    },
